{
    "name": "hyperhtmlelement-pascal",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "minify-html": "../../node_modules/.bin/html-minifier index.dist.html -o dist/index.html --collapse-whitespace --remove-comments",
        "copy-js": "cp js/* dist",
        "clean-js": "../../node_modules/.bin/replace \"import HyperHTMLElement from '../node_modules/hyperhtml-element/esm/index.js';\" \"\" dist/triangle-item.js",
        "concat-js": "../../node_modules/.bin/concat-cli -f dist/pascal-triangle.js dist/triangle-item.js -o dist/bundle_tmp.js",
        "minify-js": "../../node_modules/.bin/uglifyjs dist/bundle.js -o dist/bundle.js",
        "rollup": "../../node_modules/.bin/rollup -c",
        "prebuild": "rm -rf dist && mkdir dist",
        "build": "npm run minify-html && npm run copy-js && npm run clean-js && npm run concat-js && npm run rollup && npm run minify-js",
        "postbuild": "cd dist && rm bundle_tmp.js && rm pascal-triangle.js && rm triangle-item.js"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "hyperhtml": "^2.29.0",
        "hyperhtml-element": "^3.9.0"
    }
}